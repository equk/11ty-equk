    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "newpost": "node newpost.js"
  },
  "author": "equk <equk@users.noreply.github.com>",
  "private": true,
  "license": "MIT",
  "devDependencies": {
