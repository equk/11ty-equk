{
  "name": "11ty-equk",
  "description": "Eleventy blog for equk",
  "type": "module",
  "scripts": {
    "build": "node ./prebuild.mjs && NODE_ENV=production npx @11ty/eleventy",
    "dev": "npx @11ty/eleventy --serve --quiet",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy",
    "devdebug": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "newpost": "node newpost.js"
  },
  "author": "equk <equk@users.noreply.github.com>",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@11ty/eleventy": "3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-navigation": "^1.0.5",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@11ty/font-awesome": "1.0.1",
    "@eslint/js": "^10.0.1",
    "@types/markdown-it": "^14.1.2",
    "autoprefixer": "^10.4.24",
    "cssnano": "^7.1.2",
    "cssnano-preset-lite": "^4.0.4",
    "dotenv": "^17.3.1",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n": "^17.24.0",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "markdown-it": "^14.1.1",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-github-alerts": "^1.0.1",
    "markdown-it-task-lists": "^2.1.1",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.8.1",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.17"
  },
  "pnpm": {
    "overrides": {
      "ajv": ">=6.14.0",
      "braces": ">=3.0.3",
      "brace-expansion@<=1.1.11": "1.1.12",
      "brace-expansion@=2.0.1": "2.0.2",
      "cross-spawn": ">=7.0.5",
      "glob@<=10.4.9": "10.5.0",
      "image-size@<=1.2.0": "^1.2.1",
      "js-yaml@<=3.14.1": "3.14.2",
      "js-yaml@=4.1.0": "4.1.1",
      "micromatch": ">=4.0.8",
      "minimatch": ">=10.2.1",
      "nanoid@<=3.3.7": "^3.3.8",
      "prismjs": ">=1.30.0",
      "semver": ">=7.5.2",
      "send": ">=0.19.0",
      "ws": ">=8.17.1"
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
