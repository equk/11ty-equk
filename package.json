{
  "name": "11ty-equk",
  "description": "Eleventy blog for equk",
  "type": "module",
  "scripts": {
    "build": "node ./prebuild.mjs && NODE_ENV=production npx @11ty/eleventy",
    "dev": "npx @11ty/eleventy --serve --quiet",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy",
    "devdebug": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "newpost": "node newpost.js"
  },
  "author": "equk <equk@users.noreply.github.com>",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "3.0.0-alpha.10",
    "@11ty/eleventy-img": "^4.0.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-bundle": "^2.0.2",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@eslint/js": "^9.1.1",
    "@types/markdown-it": "^14.1.1",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.1",
    "cssnano-preset-lite": "^4.0.0",
    "dotenv": "^16.4.5",
    "eleventy-plugin-seo": "^0.5.2",
    "esbuild": "^0.20.2",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.4.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "globals": "^15.1.0",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-container": "^4.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "sanitize-html": "^2.13.0",
    "sharp": "^0.33.3",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.3"
  },
  "pnpm": {
    "overrides": {
      "semver": ">=7.5.2",
      "word-wrap": ">=1.2.4"
    }
  }
