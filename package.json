{
  "name": "11ty-equk",
  "description": "Eleventy blog for equk",
  "scripts": {
    "build": "NODE_ENV=production node ./prebuild.mjs && npx @11ty/eleventy",
    "dev": "npx @11ty/eleventy --serve --quiet",
